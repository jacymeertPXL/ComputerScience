        <Button x:Name="BtnStart" Content="Start" HorizontalAlignment="Left" Margin="477,138,0,0" VerticalAlignment="Top" Height="35" Width="76" Background="White" Click="BtnStart_Click"/>
        <Button x:Name="Btnopnieuwe" Content="Opnieuwe" HorizontalAlignment="Left" Margin="476,182,0,0" VerticalAlignment="Top" Height="35" Width="76" Background="White" Click="Btnopnieuwe_Click"/>
        <Button x:Name="BtnSluiten" Content="Sluiten" HorizontalAlignment="Left" Margin="476,225,0,0" VerticalAlignment="Top" Height="35" Width="76" Background="#FFFFF7F7" Click="BtnSluiten_Click"/>

    </Grid>
</Window>
